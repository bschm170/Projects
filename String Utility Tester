public class StringUtilTester {
    public static void main(String[] args) {
        String type = StringUtil.toString(1.0);
        System.out.println(type);

        type = StringUtil.toString(1.0f);
        System.out.println(type);

        type = StringUtil.toString(1);
        System.out.println(type);

        type = StringUtil.toString(1L);
        System.out.println(type);

        type = StringUtil.toString('1');
        System.out.println(type);

        type = StringUtil.toString(true);
        System.out.println(type);
    }
}
